gpio	eye1						// Left eye
gpio	eye2						// Right eye
gpio	pwr						// Visor servo power
gpio	pwm						// Visor service PWM
gpio	button1						// Activation button1
gpio	button2						// Activation button2
gpio	rgb						// RGB LED chain
u8	leds						// Number of LEDs in chain
u8	led.eye1		.live=1			// LED number for eye 1
u8	led.eye2		.live=1			// LED number for eye 2
u8	led.eyes	1	.live=1			// How many LEDs in eye
c1	led.eyec	C	.live=1			// Eye LED colour
u8	led.pwr			.live=1			// LED number for servo PWR
u8	led.pwm			.live=1			// LED number for servo PWM
u8	led.button1		.live=1			// LED number for button1
u8	led.button2		.live=1			// LED number for button2
u8	led.arc			.live=1			// LED number start of arc ring
u8	led.arcs		.live=1			// How many LED in arc ring
c1	led.arcc1	C	.live=1			// Arc LED colour (bright)
c1	led.arcc2	R	.live=1			// Arc LED colour (dim)
u8	led.pulse		.live=1			// LED number start of pulsing
u8	led.pulses		.live=1			// How many LED in pusling
c1	led.pulsec	W	.live=1			// Pulse LED colour
u8	led.fixed		.live=1			// LED number start of fixed LEDs
u8	led.fixeds		.live=1			// How many LED in fixed LED
c1	led.fixedc	C	.live=1			// fixed LED colour
u8	led.cylon		.live=1			// LED number start of cylon LEDs
u8	led.cylons		.live=1			// How many LED in cylon LED
c1	led.cylonc	R	.live=1			// cylon LED colour
u8	visor.close	0	.live=1			// Angle (degrees) visor close
u8	visor.open	90	.live=1			// Angle (degrees) visor open
